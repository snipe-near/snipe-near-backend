    <style>
      body { 
        background: #F3F3F5; 
        padding: 24px;
        font-family: 'Poppins', sans-serif;
      }
      container {
        background: white
        max-width: 560px;
        padding: 8px;
        border-radius: 8px;
      }
      h1 {
        font-size: 24px;
        font-weight: 700;
      }
      button {
        background: #1300ba;
        padding: 12px;
      }
      hr {
        border-style: dashed;
        border-width: 2px 0 0 0;
      }
      #footer {
        text-align: center;
      }
      #footer a {
        text-decoration: none;
        padding: 0 8px;
      }
    </style>
  </head>
  <body style="background-image:url('https://i.ibb.co/dQ78zrD/Landing-Page-3.png');background-repeat:no-repeat;background-size:cover">
    <row>
      <column>
        <img width=120 src="https://i.ibb.co/9WB1M3x/Group-7-1.png" alt="logo-black" border="0" style="margin-left:auto; margin-right:auto;margin-top:10px;margin-bottom:20px">
      </column>
    </row>
    <container style="background-color:#5f3f5d;border-radius:10px;border:1px solid #cca8b4">
      <row>
        <column style="padding: 0 12px">
